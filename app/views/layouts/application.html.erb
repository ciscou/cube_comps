<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>CubeComps</title>
    <%= csrf_meta_tags %>
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
    <![endif]-->
    <%= stylesheet_link_tag "application", :media => "all" %>
  </head>
  <body>

    <%= render "layouts/navbar" %>

    <div class="container">
      <div class="content">
        <div class="row">
          <%= flash_messages %>
          <div class="span9">
            <%= yield %>
          </div>
          <div class="span3">
            <%= render "layouts/sidebar" %>
          </div>
        </div>
      </div>
    </div>

    <%= javascript_include_tag "application" %>

  </body>
</html>
